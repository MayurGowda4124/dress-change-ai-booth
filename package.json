{
  "name": "dress-change-ai-booth-backend",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.2",
    "postcss": "^8.4.31",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cross-env NODE_OPTIONS=--no-deprecation react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "frontend": "cross-env NODE_OPTIONS=--no-deprecation react-scripts start",
    "build:clean": "rimraf node_modules/.cache build && cross-env NODE_OPTIONS=--no-deprecation react-scripts build",
    "electron": "electron .",
    "electron:build": "npm run build && electron-builder --win",
    "electron:pack": "npm run build && electron-builder --win --dir",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "dress",
    "ai",
    "booth",
    "virtual-tryon",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT",
  "proxy": "http://localhost:5000",
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5"
  },
  "build": {
    "appId": "com.aidressbooth.app",
    "productName": "AI Dress Booth",
    "extends": null,
    "directories": { "output": "release" },
    "files": ["build", "public", "server.js", "electron", "package.json", "node_modules"],
    "asarUnpack": ["server.js", "build/**", "public/**"],
    "win": { "target": ["nsis"], "signAndEditExecutables": false }
  }
}
